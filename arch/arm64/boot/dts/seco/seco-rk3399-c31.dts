
/dts-v1/;

#include "dt-bindings/pwm/pwm.h"
#include <dt-bindings/input/input.h>
#include <dt-bindings/display/drm_mipi_dsi.h>

#include "seco-rk3399.dtsi"
#include "seco-rk3399-opp.dtsi"
#include "seco-rk3399-vop-clk-set.dtsi"
#include "dt-bindings/net/ti-dp83867.h"


/ {
	model      = "Rockchip RK3399 SECO Board (C31)";
	compatible = "rockchip,seco-rk3399-c31", "rockchip,rk3399";


    chosen {
		bootargs = "earlycon=uart8250,mmio32,0xff1a0000 swiotlb=1 console=ttyFIQ0,115200n8n rw rootwait coherent_pool=1m";
		stdout-path = "serial2:115200n8";
	};


	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		drm_logo: drm-logo@00000000 {
			compatible = "rockchip,drm-logo";
			reg = <0x0 0x0 0x0 0x0>;
		};
	};


/*  __________________________________________________________________________
 * |                                                                          |
 * |                                   DEBUG                                  |
 * |__________________________________________________________________________|
 */
	fiq_debugger: fiq-debugger {
		compatible               = "rockchip,fiq-debugger";
		rockchip,serial-id       = <2>;
		rockchip,signal-irq      = <182>;
		rockchip,wake-irq        = <0>;
		rockchip,irq-mode-enable = <1>;  /* If enable uart uses irq instead of fiq */
		rockchip,baudrate        = <115200>;    /* Only 115200 and 1500000 */
		pinctrl-names            = "default";
		pinctrl-0                = <&uart2c_xfer>;
	};


/*  __________________________________________________________________________
 * |                                                                          |
 * |                                 REGULATORS                               |
 * |__________________________________________________________________________|
 */
	vcc3v3_sys: vcc3v3-sys {
		compatible              = "regulator-fixed";
		regulator-name          = "vcc3v3_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
	};


    vcc3v3_run: vcc3v3-run {
		compatible              = "regulator-fixed";
		regulator-name          = "vcc3v3_run";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		enable-active-high;
		regulator-always-on;
		vin-supply              = <&vcc3v3_sys>;
	};


	vcc1v8_run: vcc1v8-run {
		compatible              = "regulator-fixed";
		regulator-name          = "vcc1v8_run";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		enable-active-high;
		regulator-always-on;
	};


	vdd_eth: vdd-eth {
		compatible              = "regulator-fixed";
		regulator-name          = "vdd_eth";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		enable-active-high;
		regulator-always-on;
		vin-supply              = <&vcc3v3_sys>;
	};


	vcc5v0_sys: vcc5v-sys {
		compatible              = "regulator-fixed";
		regulator-name          = "vcc5v0_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
	};


	vcc12v0_sys: vcc12v-sys {
		compatible              = "regulator-fixed";
		regulator-name          = "vcc12v0_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <12000000>;
		regulator-max-microvolt = <12000000>;
	};


    sd_pwr: sd-pwr {
		compatible              = "regulator-fixed";
		gpio                    = <&gpio0 1 GPIO_ACTIVE_HIGH>;
		pinctrl-names           = "default";
		pinctrl-0               = <&sdmmc0_pwr_h>;
		regulator-name          = "vcc_sd";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		enable-active-high;
	};


	sdio_wifi_pwr: sdio-wifi-pwr {
		compatible              = "regulator-fixed";
		gpio                    = <&gpio2 RK_PD3 GPIO_ACTIVE_HIGH>;
		pinctrl-names           = "default";
		pinctrl-0               = <&sdio0_pwr_h>;
		regulator-name          = "sdio_wifi_pwr";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		enable-active-high;
        	startup-delay-us        = <1000>;
	};


    usb2_hub_host: usb2-hub-host {
		compatible              = "regulator-fixed";
		gpio                    = <&gpio1 RK_PA2 GPIO_ACTIVE_HIGH>;
		pinctrl-names           = "default";
		pinctrl-0               = <&host_hub_enable>;
		regulator-name          = "usb2_hub_host";
        regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		enable-active-down;
		regulator-always-on;
	};


	vcc5v0_host_usb1_2: vcc5v0-host-usb1-2-regulator {
		compatible              = "regulator-fixed";
		gpio                    = <&pca6416 0 GPIO_ACTIVE_HIGH>;
		regulator-name          = "vcc5v0_host_usb1_2";
        regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-always-on;
		enable-active-high;
		vin-supply              = <&vcc5v0_sys>;
	};


	vcc5v0_host_usb3_4: vcc5v0-host-usb3-4-regulator {
		compatible              = "regulator-fixed";
		gpio                    = <&pca6416 1 GPIO_ACTIVE_HIGH>;
		regulator-name          = "vcc5v0_host_usb3_4";
        regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-always-on;
		enable-active-high;
		vin-supply              = <&vcc5v0_sys>;
	};


    vcc5v0_host_usb3: vcc5v0-host-usb3-regulator {
		compatible              = "regulator-fixed";
		gpio                    = <&gpio1 RK_PA1 GPIO_ACTIVE_HIGH>;
		regulator-name          = "vcc5v0_host_usb3";
        regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-always-on;
		enable-active-high;
		vin-supply              = <&vcc5v0_sys>;
	};


	vcc_tlv320: vcc-tlv320-regulator {
		compatible              = "regulator-fixed";
		regulator-name          = "tlv320aic32x4_supply";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};


	vcc3v3_edp_vlcd_sw: vcc3v3_edp_vlcd_sw {
		compatible              = "regulator-fixed";
		pinctrl-names           = "default";
		pinctrl-0               = <&edp_vlcd_sw>;
		gpio                    = <&gpio4 RK_PD5 GPIO_ACTIVE_HIGH>;
		regulator-name          = "vcc3v3_edp_vlcd_sw";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		enable-active-high;
		vin-supply              = <&vcc3v3_sys>;
	};


	vcc12v0_edp_vbkl_sw: vcc12v0-edp-vbkl-sw {
		compatible              = "regulator-fixed";
		pinctrl-names           = "default";
		pinctrl-0               = <&edp_vblk_sw>;
		gpio                    = <&gpio4 RK_PD6 GPIO_ACTIVE_HIGH>;
		regulator-name          = "vcc3v3_edp_vbkl_sw";
		regulator-min-microvolt = <12000000>;
		regulator-max-microvolt = <12000000>;
		enable-active-high;
		vin-supply              = <&vcc12v0_sys>;
	};


	vcc_lvds_vlcd_sw: vcc-lvds-vlcd-sw {
		compatible              = "regulator-fixed";
		gpio                    = <&pca6416 5 GPIO_ACTIVE_HIGH>;
		regulator-name          = "vcc_lvds_vlcd_sw";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		enable-active-high;
		vin-supply              = <&vcc3v3_sys>;
	};


	vcc_lvds_vbkl_sw: vcc-lvds-vbkl-sw {
		compatible              = "regulator-fixed";
		gpio                    = <&pca6416 9 GPIO_ACTIVE_HIGH>;
		regulator-name          = "vcc_lvds_vbkl_sw";
		regulator-min-microvolt = <12000000>;
		regulator-max-microvolt = <12000000>;
		enable-active-high;
		vin-supply              = <&vcc12v0_sys>;
	};


	sdio_pwrseq: sdio-pwrseq {
		compatible    = "mmc-pwrseq-simple";
		clocks        = <&rk808 1>;
		clock-names   = "ext_clock";
		pinctrl-names = "default";
		pinctrl-0     = <&wifi_enable_h>;
		reset-gpios   = <&gpio3 RK_PD1 GPIO_ACTIVE_HIGH>;
	};


/*  __________________________________________________________________________
 * |                                                                          |
 * |                                     CLOCKS                               |
 * |__________________________________________________________________________|
 */
	/* 27MHz reference crystal */
	ref27M: ref27M {
        compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <27000000>;
	};


	/* 20MHz reference crystal */
	ref20M: ref20M {
        compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <20000000>;
	};
};



/*  __________________________________________________________________________
 * |                                                                          |
 * |                                   POWER                                  |
 * |__________________________________________________________________________|
 */

&pmu_io_domains {
	status = "okay";
	pmu1830-supply = <&vcc_3v0>;
};


&io_domains {
	bt656-supply    = <&vcc_3v0>; /* bt656_gpio2ab_ms */
	audio-supply    = <&vcc1v8_run>; /* audio_gpio3d4a_ms */
	sdmmc-supply    = <&sd_pwr>; /* sdmmc_gpio4b_ms */
	gpio1830-supply = <&vcc_3v0>; /* gpio1833_gpio4cd_ms */

	status          = "okay";
};
/*  __________________________________________________________________________
 * |__________________________________________________________________________|
 */



/*  __________________________________________________________________________
 * |                                                                          |
 * |                                    CPU                                   |
 * |__________________________________________________________________________|
 */
&cpu_l0 {
	cpu-supply = <&vdd_cpu_l>;
};


&cpu_l1 {
	cpu-supply = <&vdd_cpu_l>;
};


&cpu_l2 {
	cpu-supply = <&vdd_cpu_l>;
};


&cpu_l3 {
	cpu-supply = <&vdd_cpu_l>;
};


&cpu_b0 {
	cpu-supply = <&vdd_cpu_b>;
};


&cpu_b1 {
	cpu-supply = <&vdd_cpu_b>;
};


&dmc {
	status             = "okay";
	center-supply      = <&vdd_center>;
	upthreshold        = <40>;
	downdifferential   = <20>;

	system-status-freq = <
		/*system status         freq(KHz)*/
		SYS_STATUS_NORMAL       800000
		SYS_STATUS_REBOOT       528000
		SYS_STATUS_SUSPEND      200000
		SYS_STATUS_VIDEO_1080P  200000
		SYS_STATUS_VIDEO_4K     600000
		SYS_STATUS_VIDEO_4K_10B 800000
		SYS_STATUS_PERFORMANCE  800000
		SYS_STATUS_BOOST        400000
		SYS_STATUS_DUALVIEW     600000
		SYS_STATUS_ISP          600000
	>;
	vop-bw-dmc-freq = <
	/* min_bw(MB/s) max_bw(MB/s) freq(KHz) */
		0       762      200000
		763     1893     400000
		1894    3012     528000
		3013    99999    800000
	>;

	auto-min-freq = <200000>;
};
/*  __________________________________________________________________________
 * |__________________________________________________________________________|
 */


 /* __________________________________________________________________________
 * |                                                                          |
 * |                              HW ACCELERATION                             |
 * |__________________________________________________________________________|
 */

&vpu {
	status = "okay";
};

&gpu {
	status = "okay";
	mali-supply = <&vdd_gpu>;
};


&gpu_power_model {
	// for DDK r14.
	voltage       = <900>;
	frequency     = <500>;
	static-power  = <300>;
	dynamic-power = <396>;
};


&rga {
	compatible    = "rockchip,rga2";
	dev_mode      = <1>;
	reg           = <0x0 0xff680000 0x0 0x1000>;
	interrupts    = <GIC_SPI 55 IRQ_TYPE_LEVEL_HIGH 0>;
	clocks        = <&cru ACLK_RGA>, <&cru HCLK_RGA>, <&cru SCLK_RGA_CORE>;
	clock-names   = "aclk_rga", "hclk_rga", "clk_rga";
	power-domains = <&power RK3399_PD_RGA>;
	status = "okay";
};
/*  __________________________________________________________________________
 * |__________________________________________________________________________|
 */


 /*  __________________________________________________________________________
 * |                                                                          |
 * |                              A/D CONVERTER                               |
 * |__________________________________________________________________________|
 */
&saradc {
	status      = "okay";
};
/*  __________________________________________________________________________
 * |__________________________________________________________________________|
 */


/*  __________________________________________________________________________
 * |                                                                          |
 * |                        VISUAL OUTPUT PROCESSOR                           |
 * |__________________________________________________________________________|
 */
&vopl {
	status = "okay";
	assigned-clocks = <&cru DCLK_VOP1_DIV>;
	assigned-clock-parents = <&cru PLL_CPLL>;
};


&vopb_mmu {
	status = "okay";
};


&vopb {
	status = "okay";
	assigned-clocks = <&cru DCLK_VOP0_DIV>;
	assigned-clock-parents = <&cru PLL_VPLL>;
};


&vopb_mmu {
	status = "okay";
};
/*  __________________________________________________________________________
 * |__________________________________________________________________________|
 */


/*  __________________________________________________________________________
 * |                                                                          |
 * |                              eMMC/uSD/SDIO                               |
 * |__________________________________________________________________________|
 */
 /*  eMMC  */
&emmc_phy {
	status = "okay";
};

&sdhci {
	bus-width                   = <8>;
	mmc-hs400-1_8v;
	supports-emmc;
	non-removable;
	keep-power-in-suspend;
	mmc-hs400-enhanced-strobe;
	status                      = "okay";
};


/*  uSD  */
&sdmmc {
	pinctrl-names      = "default";
	pinctrl-0          = <&sdmmc_clk &sdmmc_cmd &sdmmc_cd &sdmmc_bus4>;
	clock-frequency    = <150000000>;
	clock-freq-min-max = <100000 150000000>;
	supports-sd;
	bus-width          = <4>;
	vmmc-supply        = <&sd_pwr>;
	num-slots          = <1>;
	cap-mmc-highspeed;
	cap-sd-highspeed;
	disable-wp;
	status             = "okay";
};


/*  WIFI  */
&sdio0 {
	pinctrl-names            = "default";
	pinctrl-0                = <&sdio0_bus4 &sdio0_cmd &sdio0_clk>;
	bus-width                = <4>;
	vmmc-supply              = <&sdio_wifi_pwr>;
	mmc-pwrseq               = <&sdio_pwrseq>;
	num-slots                = <1>;
	supports-sdio;
	disable-wp;
	cap-sd-highspeed;
	cap-sdio-irq;
	keep-power-in-suspend;
	non-removable;
	sd-uhs-sdr104;
	status                   = "okay";

	sd8xxx-wlan {
			status            = "okay";
			drv_mode          = <0x1>;
			cfg80211_wext     = <0xf>;
			sta_name          = "wlan";
			wfd_name          = "p2p";
			max_vir_bss       = <1>;
			cal_data_cfg      = "mrvl/cal_data.conf";
			fw_name           = "mrvl/sd8997_combo_v4.bin";
			reg_alpha2        = "US";
			p2p_enh           = <1>;
			auto_ds           = <0>;
			inact_tmo         = <3000>;
			gtk_rekey_offload = <1>;
			cfg80211_drcs     = <0>;
		};
};
