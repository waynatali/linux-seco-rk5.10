 &pinctrl {
     
    pmic {
		pmic_int_l: pmic-int-l {
			rockchip,pins =
				<1 RK_PC5 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};


	sdmmc0_pwr_h {
		sdmmc0_pwr_h: sdmmc0_pwr_h {
			rockchip,pins =
				<0 1 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};


	sdio0_pwr_h {
		sdio0_pwr_h: sdio0_pwr_h {
			rockchip,pins =
				<2 RK_PD3 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};


    gpio_expander {
		pca6416_gpios: pca6416_gpios {
			rockchip,pins =
				<2 RK_PA6 RK_FUNC_GPIO &pcfg_pull_up>,		// IRQ
				<4 RK_PD0 RK_FUNC_GPIO &pcfg_output_high>;	// RST
		};
	};


    usb2 {
		host_hub_enable: host-vbus-drv {
			rockchip,pins =
				<1 RK_PA2 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};


    fusb30x {
		fusb0_int: fusb0-int {
			rockchip,pins =
				<0 RK_PA4 RK_FUNC_GPIO &pcfg_pull_up>,
				<4 RK_PC5 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	gmac {
		rgmii_pins: rgmii-pins {
			rockchip,pins =
				/* mac_txclk */
				<3 17 RK_FUNC_1 &pcfg_pull_none_13ma>,
				/* reset */
				<3 15 RK_FUNC_1 &pcfg_output_high>,
				/* mac_rxclk */
				<3 14 RK_FUNC_1 &pcfg_pull_none>,
				/* mac_mdio */
				<3 13 RK_FUNC_1 &pcfg_pull_none>,
				/* mac_txen */
				<3 12 RK_FUNC_1 &pcfg_pull_none_13ma>,
				/* mac_rxdv */
				<3 9 RK_FUNC_1 &pcfg_pull_none>,
				/* mac_mdc */
				<3 8 RK_FUNC_1 &pcfg_pull_none>,
				/* mac_rxd1 */
				<3 7 RK_FUNC_1 &pcfg_pull_none>,
				/* mac_rxd0 */
				<3 6 RK_FUNC_1 &pcfg_pull_none>,
				/* mac_txd1 */
				<3 5 RK_FUNC_1 &pcfg_pull_none_13ma>,
				/* mac_txd0 */
				<3 4 RK_FUNC_1 &pcfg_pull_none_13ma>,
				/* mac_rxd3 */
				<3 3 RK_FUNC_1 &pcfg_pull_none>,
				/* mac_rxd2 */
				<3 2 RK_FUNC_1 &pcfg_pull_none>,
				/* mac_txd3 */
				<3 1 RK_FUNC_1 &pcfg_pull_none_13ma>,
				/* mac_txd2 */
				<3 0 RK_FUNC_1 &pcfg_pull_none_13ma>;
		};
	};


	sdio-pwrseq {
		wifi_enable_h: wifi-enable-h {
			rockchip,pins =
				<3 RK_PD1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};


	audio_i2s {
		i2s0_2ch_bus: i2s0_2ch_bus {
				rockchip,pins =
					<3 RK_PD0 RK_FUNC_1 &pcfg_pull_none>,
					<3 RK_PD2 RK_FUNC_1 &pcfg_pull_none>,
					<3 RK_PD3 RK_FUNC_1 &pcfg_pull_none>,		
					<3 RK_PD7 RK_FUNC_1 &pcfg_pull_none>;
		};


		i2s0_2ch_mclk: i2s0_2ch_mclk {
				rockchip,pins =
					<4 RK_PA0 RK_FUNC_1 &pcfg_pull_none>;
		};


		codec_audio: codec-audio {
			rockchip,pins =
				<3 RK_PD6 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};


	lcd_panel {
		edp_vlcd_sw: edp-vlcd-sw {
			rockchip,pins = 
				<4 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		edp_vblk_sw: edp-vblk-sw {
			rockchip,pins = 
				<4 RK_PD6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};


	lvds_panel {
		dsi_bridge_on: edp-bridge-on {
			rockchip,pins = 
				<3 RK_PD4 RK_FUNC_GPIO &pcfg_output_high>;
		};
	};


	gsm_modem {
		gsm_modem_pwr: gsm-modem-pwr {
			rockchip,pins = 
				<2 RK_PA2 RK_FUNC_GPIO &pcfg_pull_none>,   //pwe
				<1 RK_PC7 RK_FUNC_GPIO &pcfg_pull_none>,   //reset
				<2 RK_PA4 RK_FUNC_GPIO &pcfg_pull_none>;   //usben
		};
	};


	can1_control {
		can1_int: can1-int {
			rockchip,pins = 
				<1 RK_PA3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};


	can2_control {
		can2_int: can2-int {
			rockchip,pins = 
				<1 RK_PA4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};
